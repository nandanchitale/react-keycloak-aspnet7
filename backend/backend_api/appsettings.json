{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "keycloak": {
    "realm": "<realm_name>",
    "auth-server-url": "http://localhost:8181/",
    "ssl-required": "external",
    "resource": "<client name>",
    "verify-token-audience": true,
    "credentials": {
      "secret": "<client secret>"
    },
    "use-resource-role-mappings": true,
    "confidential-port": 0
  }
}